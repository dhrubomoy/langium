{
  "name": "langium-lsp",
  "version": "4.2.0",
  "description": "LSP services for Langium",
  "homepage": "https://langium.org",
  "engines": {
    "node": ">=20.10.0",
    "npm": ">=10.2.3"
  },
  "keywords": [
    "language",
    "dsl",
    "lsp",
    "language-server",
    "typescript"
  ],
  "license": "MIT",
  "files": [
    "lib",
    "src"
  ],
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": {
      "types": "./lib/index.d.ts",
      "import": "./lib/index.js"
    },
    "./grammar": {
      "types": "./lib/grammar/langium-grammar-module.d.ts",
      "import": "./lib/grammar/langium-grammar-module.js"
    },
    "./test": {
      "types": "./lib/test/index.d.ts",
      "import": "./lib/test/index.js"
    }
  },
  "scripts": {
    "clean": "shx rm -rf lib coverage",
    "build": "tsc"
  },
  "dependencies": {
    "langium-core": "~4.2.0",
    "langium-chevrotain": "~4.2.0",
    "vscode-languageserver": "~9.0.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/eclipse-langium/langium.git",
    "directory": "packages/langium-lsp"
  },
  "bugs": "https://github.com/eclipse-langium/langium/issues",
  "author": {
    "name": "TypeFox",
    "url": "https://www.typefox.io"
  }
}
