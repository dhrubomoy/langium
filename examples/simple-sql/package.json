{
  "name": "langium-simple-sql-dsl",
  "displayName": "Simple SQL DSL",
  "version": "4.2.0",
  "description": "SQL-like DSL built with Langium using the Lezer parser backend",
  "homepage": "https://langium.org",
  "engines": {
    "vscode": "^1.67.0"
  },
  "license": "MIT",
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [
      {
        "id": "simple-sql",
        "aliases": [
          "Simple SQL",
          "simple-sql"
        ],
        "extensions": [
          ".ssql"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "simple-sql",
        "scopeName": "source.simple-sql",
        "path": "./syntaxes/simple-sql.tmLanguage.json"
      }
    ]
  },
  "activationEvents": [
    "onLanguage:simple-sql"
  ],
  "files": [
    "out",
    "src",
    "syntaxes",
    "language-configuration.json"
  ],
  "type": "module",
  "main": "./out/extension.cjs",
  "exports": {
    ".": {
      "types": "./out/extension.d.ts",
      "default": "./out/extension.js"
    }
  },
  "scripts": {
    "build": "node esbuild.mjs",
    "watch": "node esbuild.mjs --watch",
    "langium:generate": "langium generate",
    "test": "echo 'No tests yet'"
  },
  "dependencies": {
    "langium": "~4.2.0",
    "langium-lezer": "~4.2.0",
    "vscode-languageclient": "~9.0.1",
    "vscode-languageserver": "~9.0.1"
  },
  "devDependencies": {
    "langium-cli": "~4.2.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/eclipse-langium/langium.git",
    "directory": "examples/simple-sql"
  },
  "bugs": "https://github.com/eclipse-langium/langium/issues",
  "author": {
    "name": "TypeFox",
    "url": "https://www.typefox.io"
  }
}
